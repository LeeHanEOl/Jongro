import java.util.*;

public class back2167 {

	static int arr[][]; // 왜 static?
	static int n, m;
	
	public static int addArr(int i, int j, int x, int y){
		int total = 0;
		int start, end;
		
		for(int u = (i-1); u < x; u++){
			start = 0; end = m;
			
			if((u == i-1) && (i != x)){ start = j-1; }
			else if((u == i-1) && (i == x)){ start = j-1; end = y; }
			else if(u == x-1){ end = y; }
			
			for(int v = start; v < end; v++){
				total += arr[u][v];
				//System.out.println(u + " " + v + " " + arr[u][v] + " " + total);
			}
		}
		return total;
	};
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		
		//System.out.print("숫자를 두 개 입력하세요: ");
		n = s.nextInt();
		m = s.nextInt();
		
		//System.out.println("배열을 입력하세요");
		arr = new int[n][m];
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				arr[i][j] = s.nextInt();
			}
		}
	

		//System.out.print("몇 개 구할? ");
		int k = s.nextInt();
		int i, j, x, y;
		
		int total = 0;
		
		for(int u = 0; u < k; u++){
			//System.out.println("input i,j,x,y");
			i = s.nextInt();
			j = s.nextInt();
			x = s.nextInt();
			y = s.nextInt();
			
			total = addArr(i, j, x, y);
			//System.out.println("total: " + total);
			total = 0;
		}
		
	}

}
