import java.util.*;

public class back2167 {

	static int arr[][]; // 왜 static?
	static int n, m;
	
	public static int addArr(int i, int j, int x, int y){
		int total = 0;
		int count = 1;
		
		for(int u = i; u <= x; u++){
			for(int v = j; v <= y; v++){
				total += arr[u][v];
				System.out.println("arr[" + u + "][" + v + "]: " + arr[u][v] +
						"\n" + count + ".total: " + total);
			}
		}
		return total;
	};
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		
		System.out.print("숫자를 두 개 입력하세요: ");
		n = s.nextInt();
		m = s.nextInt();
		
		System.out.println("배열을 입력하세요");
		arr = new int[n][m];
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				arr[i][j] = s.nextInt();
			}
		}
	

		System.out.print("몇 개 구할? ");
		int k = s.nextInt();
		int i, j, x, y;
		
		int total = 0;
		
		for(int u = 0; u < k; u++){
			System.out.println("input i,j,x,y");
			i = s.nextInt();
			j = s.nextInt();
			x = s.nextInt();
			y = s.nextInt();
			
			total = addArr(i, j, x, y);
			System.out.println("total: " + total);
			total = 0;
		}
		
	}

}
