import java.util.*;

public class back2167 {

	static int arr[][]; // 왜 static?
	static int n, m;
	
	public static int addArr(int i, int j, int x, int y){
		int total = 0;
		int count = 1;
		int start, end;
		
		for(int u = (i-1); u < x; u++){
			start = 0; end = m;
			
			if((i == x) && (j == y)){ total = arr[i][j]; break; }
			else if(u == i-1){ start = j-1; }
			else if(u == x-1){ end = y; }
			
			System.out.println("from " + (i-1) + " to " + x);
			System.out.println("from " + start + " to " + end);
			
			for(int v = start; v < end; v++){
				System.out.println(u + " " + v + " " + arr[u][v]);
				total += arr[u][v];
				System.out.println("total: " + total);
			}
		}
		return total;
	};
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		
		System.out.print("숫자를 두 개 입력하세요: ");
		n = s.nextInt();
		m = s.nextInt();
		
		System.out.println("배열을 입력하세요");
		arr = new int[n][m];
		for(int i = 0; i < n; i++){
			for(int j = 0; j < m; j++){
				arr[i][j] = s.nextInt();
			}
		}
	

		System.out.print("몇 개 구할? ");
		int k = s.nextInt();
		int i, j, x, y;
		
		int total = 0;
		
		for(int u = 0; u < k; u++){
			System.out.println("input i,j,x,y");
			i = s.nextInt();
			j = s.nextInt();
			x = s.nextInt();
			y = s.nextInt();
			
			total = addArr(i, j, x, y);
			System.out.println("total: " + total);
			total = 0;
		}
		
	}

}
